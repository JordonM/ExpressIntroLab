<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title> Express Lab </title>
</head>
<body>
    <h1>Student Attendance</h1>
    <ul>
      <% students.forEach(function(t) { %>
        <li>
          <%= t.student %>
            - 
          <%= t.absent  ? 'absent' : 'present' %>
        </li>
      <% }); %>
    </ul>
  </body>
</html>